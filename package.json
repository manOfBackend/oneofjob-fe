{
  "name": "apps-oneofjob-frontend",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "build": "remix vite:build",
    "build:netlify": "pnpm run typecheck && pnpm run lint && pnpm run build",
    "build:mocks": "ENABLE_MOCKS=true remix vite:build",
    "preview": "remix-serve ./build/server/index.js",
    "install:frozen": "pnpm install --frozen-lockfile",
    "install:clean": "rm -rf node_modules && pnpm install",
    "dev": "remix vite:dev",
    "dev:mocks": "ENABLE_MOCKS=true remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "update:deps": "pnpm update --latest",
    "check:deps": "pnpm audit",
    "typecheck": "tsc"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": ["react", "react-dom"]
    }
  },
  "dependencies": {
    "@remix-run/node": "^2.16.1",
    "@remix-run/react": "^2.16.1",
    "@remix-run/serve": "^2.16.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.1",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "msw": "^2.7.3",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=10.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
